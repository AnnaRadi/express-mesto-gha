{
  "env": {
      "node": true,
      "commonjs": true
  },
  "extends": "airbnb-base",
  "rules": {
      "indent": [
          "error",
          "tab"
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ]
  },
  "parserOptions": {
      "ecmaVersion": 2015
  }
}